generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("POSTGRES_PRISMA_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}

model Url {
  id        String   @id @default(cuid())
  url       String
  nanoId    String
  status    String
  isCopied  Boolean
  createdAt DateTime @default(now()) @map("created_at")
  updatedAt DateTime @updatedAt @map("updated_at")
  survey    Survey?
  total     Totals   @relation(fields: [totalId], references: [id])
  totalId   String
}

model Survey {
  id           String   @id @default(cuid())
  address      String   @unique
  gender       String
  age          String
  race         String
  job          String
  location     String
  mushroom_exp String
  computer_exp String
  crypto_exp   String
  nft_exp      String
  createdAt    DateTime @default(now()) @map("created_at")
  url          Url      @relation(fields: [urlId], references: [id])
  urlId        String   @unique
  total        Totals   @relation(fields: [totalId], references: [id])
  totalId      String
}

model Totals {
  id         String   @id @default(cuid())
  table_name String
  total      Int
  urlIds     Url[]
  surveyIds  Survey[]
}
